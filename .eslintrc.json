{
    "env": {
        "browser": true,
        "es2019": true,
        "googleappsscript/googleappsscript": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "plugins": [
        "import",
        "googleappsscript"
    ],
    "rules": {
        "no-mixed-spaces-and-tabs": 0,
        "no-unused-vars": 0,
        "no-constant-condition": 0,
        "no-empty": 0,
        "import/no-unresolved": [
            "error",
            {
                "ignore": [
                    "^./"
                ]
            }
        ]
    }
}